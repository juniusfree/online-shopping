{
  "compilerOptions": {
    "baseUrl": "src",
    "target": "es5",
    "paths": {
      "components/*": [
        "components/*"
      ],
      "hooks/*": [
        "hooks/*"
      ],
      "icons/*": [
        "icons/*"
      ],
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": [
    "src"
  ]
}
